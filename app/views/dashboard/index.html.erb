<%= flash_messages %>
<div class="block">
  <header class="header-group">
    <%= react_component("Heading", {name: "Music games"}) %>
  </header>
  <div class="row">
    <div class="col-md-12">
      <h1 x-data="{ message: 'I ❤️ Alpine' }" x-text="message"></h1>
      <div x-data="{ count: 0 }">
        <button x-on:click="count++">Increment</button>
        <span x-text="count"></span>
      </div>
    </div>
  </div>
</div>
